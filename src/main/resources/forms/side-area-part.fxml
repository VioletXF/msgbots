<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<?import org.beuwi.msgbots.platform.gui.control.BotView?>
<?import org.beuwi.msgbots.platform.gui.control.Tab?>
<?import org.beuwi.msgbots.platform.gui.control.TabPane?>

<AnchorPane prefWidth="220.0" styleClass="side-area" stylesheets="@../styles/side-area-part.css" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TabPane type="SYSTEM" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
			<tabs>
				<Tab fx:id="tabBotList" title="BOT LIST" closable="false">
					<content>
						<StackPane styleClass="bots-tab">
							<BotView fx:id="lsvBotView"> </BotView>
						</StackPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
		<Pane fx:id="stpResizeBar" prefWidth="5.0" AnchorPane.topAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0">
			<cursor>
				<Cursor fx:constant="E_RESIZE" />
			</cursor>
		</Pane>
	</children>
</AnchorPane>