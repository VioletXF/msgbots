<AnchorPane minWidth="220.0" styleClass="active-area" stylesheets="@../styles/ActiveAreaPart.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TabPane side="LEFT" tabClosingPolicy="UNAVAILABLE" tabMinHeight="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab>
               <content>
                  <VBox styleClass="bots-tab" />
               </content>
          </Tab>
          <Tab>
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="debug-tab" />
               </content>
          </Tab>
        </tabs>
      </TabPane>
      <Pane prefWidth="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <cursor>
            <Cursor fx:constant="E_RESIZE" />
         </cursor>
      </Pane>
   </children>
</AnchorPane>
