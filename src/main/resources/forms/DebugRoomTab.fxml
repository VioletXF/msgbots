<VBox alignment="CENTER" cacheShape="false" centerShape="false" minWidth="350.0" prefHeight="800.0" prefWidth="1000.0" scaleShape="false" stylesheets="@../styles/DebugRoomTab.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="listView" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
      <AnchorPane maxHeight="60.0" VBox.vgrow="NEVER">
         <children>
            <TextArea fx:id="textArea" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="70.0" AnchorPane.topAnchor="0.0" />
