<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>

<?import org.beuwi.msgbots.platform.gui.control.Button?>
<?import org.beuwi.msgbots.platform.gui.control.HBox?>
<?import org.beuwi.msgbots.platform.gui.control.TextField?>

<AnchorPane prefHeight="120.0" prefWidth="450.0" styleClass="import-dialog" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox prefHeight="30.0" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<TextField fx:id="txfScriptName" editable="false" HBox.hgrow="ALWAYS" />
				<Button fx:id="btnScriptOpen" styled="true" prefWidth="35.0" text="..." />
			</children>
		</HBox>
		<CheckBox fx:id="chkIsUnified" text="통합된 매개변수를 사용합니다." AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" />
		<CheckBox fx:id="chkIsOffError" selected="true" text="런타임 에러 시 자동으로 봇의 전원을 끕니다. (권장)" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
	</children>
</AnchorPane>