<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" cacheShape="false" centerShape="false" minWidth="350.0" prefHeight="800.0" prefWidth="1000.0" scaleShape="false" stylesheets="@../styles/debug-room-tab.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="listView" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
      <HBox>
         <children>
            <TextArea fx:id="textArea" HBox.hgrow="ALWAYS" />
            <Button fx:id="button" disable="true" />
         </children>
      </HBox>
   </children>
</VBox>
